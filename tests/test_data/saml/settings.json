{
    "strict": true,
    "debug": true,
    "sp": {
        "entityId": "example-client",
        "assertionConsumerService": {
            "url": "http://localhost:8000/?acs",
            "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST"
        },
        "singleLogoutService": {
            "url": "http://localhost:8000/?sls",
            "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
        },
        "NameIDFormat": "urn:oasis:names:tc:SAML:1.1:nameid-format:unspecified",
        "__sp_keys": "Create keys and upload: 'openssl req -new -x509 -days 3652 -nodes -out sp.crt -keyout sp.key'",
        "x509cert": "",
        "privateKey": ""
    },
    "idp": {
        "entityId": "http://localhost:8080/auth/realms/example-realm",
        "singleSignOnService": {
            "url": "http://localhost:8080/auth/realms/example-realm/protocol/saml",
            "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
        },
        "singleLogoutService": {
            "url": "http://localhost:8080/auth/realms/example-realm/protocol/saml",
            "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
        },
        "x509cert": ""
    }
}
